{
  "name": "bluewhale-web",
  "version": "0.0.1",
  "description": "The bluewhale web management platform for docker.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "gulp dev",
    "dev:web": "cd src/web-front && ng build --extract-css --watch",
    "lint:web": "cd src/web-front && ng lint",
    "build:api": "gulp build",
    "build:web": "cd src/web-front && ng build --prod --aot",
    "clean": "gulp clean",
    "build": "npm run clean && npm run build:api && npm run build:web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/golang-community/bluewhale-web.git"
  },
